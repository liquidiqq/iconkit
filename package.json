{
	"name": "@liquidiqq/iconkit",
	"description": "Handpicked community icons for the Svelte ecosystem.",
	"version": "1.2.4",
	"author": {
		"name": "Liquidiqq"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"icon",
		"icons",
		"svg",
		"community",
		"open-source",
		"typescript"
	],
	"license": "MIT",
	"homepage": "https://github.com/liquidiqq/iconkit",
	"repository": {
		"type": "git",
		"url": "https://github.com/liquidiqq/iconkit.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package -o package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.32.3",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.7",
		"@sveltejs/package": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.0",
		"vitest": "^0.30.1"
	},
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		"./Icon.svelte": {
			"types": "./package/Icon.svelte.d.ts",
			"svelte": "./package/Icon.svelte",
			"default": "./package/Icon.svelte"
		},
		"./icons": {
			"types": "./package/icons.d.ts",
			"default": "./package/icons.js"
		},
		".": {
			"types": "./package/index.d.ts",
			"svelte": "./package/index.js",
			"default": "./package/index.js"
		},
		"./loading-circle.css": "./package/loading-circle.css"
	},
	"files": [
		"package"
	],
	"svelte": "./package/index.js",
	"typesVersions": {
		">4.0": {
			"Icon.svelte": [
				"./package/Icon.svelte.d.ts"
			],
			"icons": [
				"./package/icons.d.ts"
			],
			"index": [
				"./package/index.d.ts"
			]
		}
	}
}
